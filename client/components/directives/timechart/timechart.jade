.chart-controls
  timespanselect(on-change='changeTimespan(fromTime)')
.chart-container.timechart-container.animated.animated-delay.ng-hide(ng-hide='chartLoading')
  highchart#timechart(ng-style='timeChartStyle', config='timechartConfig')
.loading-container.timechart-container(ng-show='chartLoading')
  .loading
    span.loading-spinner
    p.loading-text Loading Chart...
.row
  .col-md-12.chart-controls
    button.btn.btn-sm.btn-primary(ng-click='hideAll()') Hide All
    button.btn.btn-sm.btn-primary(ng-click='showAll()') Show All

