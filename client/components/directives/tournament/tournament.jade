.bs-callout(ng-class='{"bs-callout-info": role == "editor", "bs-callout-warning": role == "admin", "bs-callout-primary": !role}')
  h4(ng-bind='tournament.name')
  p {{tournament.members.length}} Members
  p Created on {{tournament.created | date:"MMMM d, y"}}
  span.label.label-danger(ng-show='role') {{role|capitalize}}
